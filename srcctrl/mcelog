. ./functions

GITURL="git://git.kernel.org/pub/scm/utils/cpu/mce/mcelog.git"
DIRNAME='mcelog'

BINARIES="\
mcelog"

fetch () {
  #git_fetch ${GITURL} ${DIRNAME}
  return
}

unpack () {
  # nothing to do here..
  return
}

build () {
  checkdone
  cd ${SRCDIR}/${DIRNAME}
  make clean
  make
  if [ $? != 0 ]; then
    return 1
  fi
  markdone
}

$1 $2

if [ $? != 0 ]; then
  printf 'Source control script %s failed on function %s' $0 $1
  exit 1
fi

